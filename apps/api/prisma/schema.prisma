generator client {
  provider = "prisma-client-js"
  output   = "./node_modules/@prisma/client"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_DATABASE_URL")
}

model Account {
  id    String @id @default(uuid())
  users User[]
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  name     String
  password String

  account   Account @relation(fields: [accountId], references: [id])
  accountId String

  @@map("users")
}
